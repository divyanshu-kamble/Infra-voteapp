pipeline {
    agent {
    label 'Deploy'
}

    // define environment variable ENVIRONMENT
    
    stages {
        stage('Deploying pods') {
            steps {
                withCredentials([usernamePassword(credentialsId: 'docker', passwordVariable: 'password', usernameVariable: 'username')]) {
                sh '''
                    kubectl apply -f kubernetes 
                    
                '''
            }
        }
    }
}
}
