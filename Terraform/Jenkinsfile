pipeline {
  agent any
     stages {
        stage('terrafrom script') {
            steps {
                withCredentials([aws(accessKeyVariable: 'AWS_ACCESS_KEY_ID', secretKeyVariable: 'AWS_SECRET_ACCESS_KEY', credentialsId: 'AWS-1')]) {
                    sh '''
                        cd Terraform
                        pwd
                        terraform -v
                        terraform init
                        terraform plan

                    '''
          }
       }
    }
  }
}
