pipeline {
  agent any
     stages {
        stage('terrafrom script') {
            steps {
                withAWS([credentials (credentialsId: 'aws-creds', passwordVariable: 'password', usernameVariable: 'username')]) {
                    sh '''
                        cd Terraform
                        pwd
                        terrafrom --version
                    '''
          }
       }
    }
  }
}
