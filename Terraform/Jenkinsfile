pipeline {
  agent any
     stages {
        stage('terrafrom script') {
            steps {
                withCredentials([$class: 'AmazonWebServicesCredentialsBinding', accessKeyVariable: 'AWS_ACCESS_KEY_ID', secretKeyVariable: 'AWS_SECRET_ACCESS_KEY', credentialsId: '${AWS_ACCOUNT}']) {
                    sh '''
                        cd Terraform
                        pwd
                        terrafrom --version
                    '''
          }
       }
    }
  }
}
